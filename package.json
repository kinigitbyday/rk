{
  "name": "rk",
  "version": "0.0.0",
  "description": "Command line tools",
  "author": "Anton Kropp @devshorts",
  "bin": {
    "rk": "./bin/run"
  },
  "homepage": "https://github.com/devshorts/rk",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "kinigitbyday/rk",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/prompts": "^5.0.5",
    "@oclif/core": "^1.7.0",
    "@oclif/plugin-help": "^5",
    "@octokit/rest": "^18.12.0",
    "@shortcut/client": "^1.1.0",
    "add": "^2.0.6",
    "lodash": "^4.17.21",
    "octokit": "^3.2.1",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@octokit/types": "^9.2.1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "fs-extra": "^11.1.0",
    "gifwrap": "^0.10.1",
    "globby": "^11",
    "jimp": "^0.9.3",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.4.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "rk",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": ":"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "speed-up": "oclif manifest",
    "regen-readme": "oclif readme",
    "finalize": "yarn build && yarn regen-readme && yarn speed-up && git add README.md oclif.manifest.json"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "bugs": "https://github.com/devshorts/rk/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
